# .env.example

# OpenAI에서 발급받은 API 키 (예: sk-proj-xxxxx...)
OPENAI_API_KEY=your_openai_key_here

# Neo4j Aura 연결 URI (예: neo4j+s://xxxx.databases.neo4j.io)
NEO4J_URI=neo4j+s://your-neo4j-uri

# 사용자 이름 (보통 'neo4j')
NEO4J_AUTH_USERNAME=neo4j

# 비밀번호 (Neo4j Aura 비밀번호)
NEO4J_AUTH_PASSWORD=your_password_here

# 자연어→Cypher 변환 서버 (FastAPI MCP 서버 주소)
MCP_SERVER_ENDPOINT=http://localhost:8000/generate-query
